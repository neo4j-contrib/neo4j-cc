{
  "name": "@neo4j-cc/cc-pipe",
  "author": "Andreas Kollegger",
  "version": "0.1.0",
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/cc-batch.esm.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test",
    "test:watch": "tsdx test --watch",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "fetch": "run-s fetch:*",
    "fetch:gh": "ts-node scripts/fetch-gh-repos.ts > data/projects.json",
    "fetch:gists": "ts-node scripts/fetch-gists-graphql.ts > data/gists.json",
    "fetch:people": "ts-node scripts/fetch-discourse-users.ts > data/people.json",
    "fetch:meetups": "ts-node scripts/fetch-meetups-groups.ts > data/meetups.json",
    "fetch:awesome": "echo 'Fetch from https://github.com/neueda/awesome-neo4j/blob/master/README.md'",
    "fetch:free": "ts-node scripts/fetch-free-dev.ts > data/free-dev.json",
    "fetch:devto": "ts-node scripts/devto-to-orbit.ts"
  },
  "jest": {
    "setupFilesAfterEnv": ["@relmify/jest-fp-ts"]
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "size-limit": [
    {
      "path": "dist/cc-batch.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/cc-batch.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@relmify/jest-fp-ts": "^1.1.1",
    "@size-limit/preset-small-lib": "^4.10.2",
    "@types/luxon": "^1.26.3",
    "husky": "^6.0.0",
    "size-limit": "^4.10.2",
    "tsdx": "^0.14.1",
    "tslib": "^2.2.0",
    "typescript": "^4.2.4"
  },
  "dependencies": {
    "@effect-ts/core": "",
    "@effect-ts/jest": "",
    "@effect-ts/morphic": "",
    "@effect-ts/node": "",
    "@effect-ts/query": "",
    "@urql/core": "^2.0.0",
    "axios": "^0.21.1",
    "axios-rate-limit": "^1.3.0",
    "dotenv": "^8.2.0",
    "fp-ts": "^2.10.2",
    "fp-ts-rxjs": "^0.6.13",
    "graphql": "^15.5.0",
    "luxon": "^1.26.0",
    "rxjs": "^6.6.7",
    "urql": "^2.0.2",
    "yargs": "^16.2.0"
  }
}
