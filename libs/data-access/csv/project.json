{
  "name": "data-access-csv",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/data-access/csv/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/data-access/csv",
        "main": "libs/data-access/csv/src/index.ts",
        "tsConfig": "libs/data-access/csv/tsconfig.lib.json",
        "assets": ["libs/data-access/csv/*.md"]
      }
    },
    "publish": {
      "executor": "nx:run-commands",
      "options": {
        "command": "node tools/scripts/publish.mjs data-access-csv {args.ver} {args.tag}"
      },
      "dependsOn": [
        {
          "projects": "self",
          "target": "build"
        }
      ]
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/data-access/csv/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/data-access/csv"],
      "options": {
        "jestConfig": "libs/data-access/csv/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
