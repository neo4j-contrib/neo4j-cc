{
  "name": "access-fs",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/access/fs/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/access/fs",
        "main": "libs/access/fs/src/index.ts",
        "tsConfig": "libs/access/fs/tsconfig.lib.json",
        "assets": ["libs/access/fs/*.md"]
      }
    },
    "publish": {
      "executor": "nx:run-commands",
      "options": {
        "command": "node tools/scripts/publish.mjs data-access-fs {args.ver} {args.tag}"
      },
      "dependsOn": ["build"]
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/access/fs/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/vite:test",
      "outputs": ["coverage/libs/access/fs"],
      "options": {
        "passWithNoTests": true,
        "reportsDirectory": "../../../coverage/libs/access/fs"
      }
    }
  },
  "tags": []
}
