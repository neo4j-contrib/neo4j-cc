{
  "name": "pipe-khoros-to-discourse",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/pipe/khoros-to-discourse/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/js:tsc",
      "outputs": [
        "{options.outputPath}"
      ],
      "options": {
        "outputPath": "dist/libs/pipe/khoros-to-discourse",
        "main": "libs/pipe/khoros-to-discourse/src/index.ts",
        "tsConfig": "libs/pipe/khoros-to-discourse/tsconfig.lib.json",
        "assets": [
          "libs/pipe/khoros-to-discourse/*.md"
        ]
      }
    },
    "publish": {
      "executor": "nx:run-commands",
      "options": {
        "command": "node tools/scripts/publish.mjs pipe-khoros-to-discourse {args.ver} {args.tag}"
      },
      "dependsOn": [
        "build"
      ]
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": [
        "{options.outputFile}"
      ],
      "options": {
        "lintFilePatterns": [
          "libs/pipe/khoros-to-discourse/**/*.ts"
        ]
      }
    },
    "test": {
      "executor": "@nrwl/vite:test",
      "outputs": [
        "coverage/libs/pipe/khoros-to-discourse"
      ],
      "options": {
        "passWithNoTests": true,
        "reportsDirectory": "../../../coverage/libs/pipe/khoros-to-discourse"
      }
    }
  },
  "tags": []
}
