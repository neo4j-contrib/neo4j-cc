{
  "name": "example-effect-ts",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/example/effect-ts/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/example/effect-ts",
        "main": "libs/example/effect-ts/src/index.ts",
        "tsConfig": "libs/example/effect-ts/tsconfig.lib.json",
        "assets": ["libs/example/effect-ts/*.md"]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/example/effect-ts/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "libs/example/effect-ts/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
