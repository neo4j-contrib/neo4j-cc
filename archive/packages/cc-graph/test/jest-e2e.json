{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "@cc/cc-orbit/(.*)": "<rootDir>/../libs/cc-orbit/src/$1",
    "@cc/cc-orbit": "<rootDir>/../libs/cc-orbit/src"
  }
}