{
  "compileOnSave": false,
  "compilerOptions": {
    "rootDir": ".",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es2015",
    "module": "esnext",
    "lib": ["es2017", "dom"],
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "baseUrl": ".",
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "paths": {
      "@neo4j-cc/data-access-cc": ["libs/data-access/cc/src/index.ts"],
      "@neo4j-cc/data-access-discourse": [
        "libs/data-access/discourse/src/index.ts"
      ],
      "@neo4j-cc/data-access-github": ["libs/data-access/github/src/index.ts"],
      "@neo4j-cc/data-access-http": ["libs/data-access/http/src/index.ts"],
      "@neo4j-cc/data-access-khoros": ["libs/data-access/khoros/src/index.ts"],
      "@neo4j-cc/data-access-neo4j": ["libs/data-access/neo4j/src/index.ts"],
      "@neo4j-cc/data-access-csv": ["libs/data-access/csv/src/index.ts"],
      "@neo4j-cc/example/effect-service": [
        "libs/example/effect-service/src/index.ts"
      ],
      "@neo4j-cc/feature/ninja-leaderboard": [
        "libs/feature/ninja-leaderboard/src/index.ts"
      ],
      "@neo4j-cc/feature/person": ["libs/feature/person/src/index.ts"],
      "@neo4j-cc/feature/release-notes": [
        "libs/feature/release-notes/src/index.ts"
      ],
      "@neo4j-cc/feature/twin4j": ["libs/feature/twin4j/src/index.ts"],
      "@neo4j-cc/graphql-types": ["libs/graphql-types/src/index.ts"],
      "@neo4j-cc/model": ["libs/model/src/index.ts"],
      "@neo4j-cc/nest/neo4j-dbms": ["libs/nest/neo4j-dbms/src/index.ts"],
      "@neo4j-cc/prelude": ["libs/prelude/src/index.ts"],
      "@neo4j-cc/ui": ["libs/ui/src/index.ts"],
      "@neo4j-cc/util-arbitrary": ["libs/util/arbitrary/src/index.ts"]
    }
  },
  "exclude": ["node_modules", "tmp"]
}
