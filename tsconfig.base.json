{
  "compileOnSave": false,
  "compilerOptions": {
    "rootDir": ".",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es2015",
    "module": "esnext",
    "lib": ["es2017", "dom"],
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "baseUrl": ".",
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "paths": {
      "@neo4j-cc/access-cc": ["libs/access/cc/src/index.ts"],
      "@neo4j-cc/access-csv": ["libs/access/csv/src/index.ts"],
      "@neo4j-cc/access-cypher": ["libs/access/cypher/src/index.ts"],
      "@neo4j-cc/access-discourse": ["libs/access/discourse/src/index.ts"],
      "@neo4j-cc/access-fs": ["libs/access/fs/src/index.ts"],
      "@neo4j-cc/access-github": ["libs/access/github/src/index.ts"],
      "@neo4j-cc/access-http": ["libs/access/http/src/index.ts"],
      "@neo4j-cc/access-khoros": ["libs/access/khoros/src/index.ts"],
      "@neo4j-cc/access-neo4j": ["libs/access/neo4j/src/index.ts"],
      "@neo4j-cc/example/effect-ts": ["libs/example/effect-ts/src/index.ts"],
      "@neo4j-cc/graphql-types": ["libs/graphql-types/src/index.ts"],
      "@neo4j-cc/model": ["libs/model/src/index.ts"],
      "@neo4j-cc/nest/neo4j-dbms": ["libs/nest/neo4j-dbms/src/index.ts"],
      "@neo4j-cc/pipe-khoros-to-discourse": [
        "libs/pipe/khoros-to-discourse/src/index.ts"
      ],
      "@neo4j-cc/prelude": ["libs/prelude/src/index.ts"],
      "@neo4j-cc/ui": ["libs/ui/src/index.ts"],
      "@neo4j-cc/util-arbitrary": ["libs/util/arbitrary/src/index.ts"]
    }
  },
  "exclude": ["node_modules", "tmp"]
}
